{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'jellyfish_lang'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=jellyfish_lang"
                ],
                "filter": {
                    "name": "jellyfish_lang",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'jellyfish_lang'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=jellyfish_lang",
                    "--package=jellyfish_lang"
                ],
                "filter": {
                    "name": "jellyfish_lang",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'jellyfish_lang'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=jellyfish_lang",
                    "--package=jellyfish_lang"
                ],
                "filter": {
                    "name": "jellyfish_lang",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}